[run]
branch = True
omit =
    */test_*.py
    */tests/*
    */__init__.py

[report]
exclude_lines =
    # Ignore print statements
    print\(
    # Ignore import statements and constants
    import
    from
    pass
    if __name__ == .__main__.:
    pragma: no cover
omit =
    */test_*.py
    */tests/*
    */__init__.py
